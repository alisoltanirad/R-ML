get_dataset <- function(){
data_set = read.csv('Mall_Customers.csv')[4:5]
split = sample.split(data_set$DependentVariable, SplitRatio=0.8)
train_set = scale(subset(data_set, split==True))
test_set = scale(subset(data_set, split==False))
data = c(data_set, train_set, test_set)
names(data) = c('data_set', 'train_set', 'test_set')
return(data)
}
get_cluster_numbers <- function(data){
wcss = vector()
for (n in 10) wcss[n] = sum(kmeans(data$data_set, n)$whitinss)
plot(1:10, wcss, type='b', main=paste('The Elbow Method'),
xlab='Number of Clusters', ylab='WCSS')
}
get_cluster_numbers(get_dataset())
